<%- include('./head') %>
<h1><%= title %></h1>
<ul>
  <% if(author_list.length){ author_list.forEach(function(author){ %>
  <li>
    <a href=<%= author.url%>><%= author.name%> </a> <%= author.date_of_birth%> - <%= author.date_of_death%> 
  </li>
  <% })} else{ %>
  <p>There are no authors.</p>
  <% } %>
</ul>
